<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quizofgot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.activity.JogadorActivity"></activity>
        <activity android:name=".activity.activity.ConfActivity" />
        <activity android:name=".activity.activity.RecordsActivity" />
        <activity android:name=".activity.activity.ResultadoActivity" />
        <activity android:name=".activity.activity.QuizActivity" />
        <activity android:name=".activity.activity.DescriptionActivity" />
        <activity android:name=".activity.activity.TemasActivity" />
        <activity android:name=".activity.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>